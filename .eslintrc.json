{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off",
    "arrow-body-style": 0,
    "react/jsx-props-no-spreading": "off",
    "import/prefer-default-export": 0,
    "react/no-array-index-key": 0
  }
}
